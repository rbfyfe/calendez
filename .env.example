# Google OAuth (from Google Cloud Console)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Auth.js secret (generate with: openssl rand -base64 32)
AUTH_SECRET=

# The Google email that should have admin access
OWNER_EMAIL=

# Upstash Redis (from Vercel Storage or Upstash dashboard)
# Optional — without Redis, the app uses defaults from calendez.config.defaults.ts
# and requires ENCRYPTED_OWNER_REFRESH_TOKEN for production token storage
KV_REST_API_URL=
KV_REST_API_TOKEN=

# Encrypted refresh token (only needed if NOT using Redis)
# Generated automatically — sign in at /admin and copy the value shown
ENCRYPTED_OWNER_REFRESH_TOKEN=
